package rest4;

import javax.ws.rs.*;
import javax.ws.rs.core.MediaType;

@Path("/v1/status/{varX}")
public class vb_status {

	@GET
	@Produces(MediaType.TEXT_HTML)
	public String returnTitle(@PathParam("varX") String varx){
		if(varx == "kurwa")
			varx = "eeee tak brzydko";
		return "<p> JAVA WEB SERVICE kurwa </p> "+ varx;
	}
	
}
