package rest4;

import javax.ws.rs.*;
import javax.ws.rs.core.MediaType;

@Path("/v1/status/{varX}")
public class vb_status {

	@GET
	@Produces(MediaType.TEXT_HTML)
	public String returnTitle(@PathParam("varX") Integer varx){
		if(varx == 1)
			varx = 2;
		return "<p> JAVA WEB SERVICE kurwa </p> "+ varx.toString();
	}
	
}
