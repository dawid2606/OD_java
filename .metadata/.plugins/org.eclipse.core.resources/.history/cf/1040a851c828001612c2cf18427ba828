package rest4;

import javax.ws.rs.*;
import javax.ws.rs.core.MediaType;

@Path("/v1/status/{varX}")
public class vb_status {

	@GET
	@Produces(MediaType.APPLICATION_JSON)
	public User returnTitle(@PathParam("varX") Integer varx){
		if(varx == 1)
			return new User(1, "Ola");
		else
			return new User(2, "Dawid");
	}
	
}
